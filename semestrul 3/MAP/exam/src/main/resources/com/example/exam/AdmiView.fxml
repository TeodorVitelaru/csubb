<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.exam.controller.AdminController"
            prefHeight="400.0" prefWidth="600.0">
    <VBox>
        <HBox>
            <Label text="Name:"/>
            <TextField fx:id="nameField" promptText="Enter first name"/>
        </HBox>
        <HBox>
            <Label text="durationMInutes:"/>
            <TextField fx:id="durationMinutesField" promptText="Enter first name"/>
        </HBox>
        <HBox>
            <Label text="start date:"/>
            <TextField fx:id="startDateField" promptText="Enter first name"/>
        </HBox>
        <HBox>
            <Label text="nr locuri:"/>
            <TextField fx:id="numberOfSeatsField" promptText="Enter first name"/>
        </HBox>
        <Button onAction="#handleAdd" text="Add"/>
    </VBox>

</AnchorPane>

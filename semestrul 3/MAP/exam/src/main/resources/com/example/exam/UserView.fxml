<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.exam.controller.UserController"
            prefHeight="400.0" prefWidth="600.0">

    <VBox>
        <Label text="Show uri"/>
        <TableView fx:id="tableViewShows" minHeight="-Infinity" minWidth="-Infinity" prefHeight="181.0" prefWidth="700.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columns>
                <TableColumn fx:id="tableColumnShowId" prefWidth="150.0" text="show Id" />
                <TableColumn fx:id="tableColumnName" prefWidth="150.0" text="name show:" />
                <TableColumn fx:id="tableColumnDurationMinutes" prefWidth="150.0" text="duration minutes" />
                <TableColumn fx:id="tableColumnNumberOfSeats" prefWidth="150.0" text="Nr locuri" />
                <TableColumn fx:id="tableColumnStartDate" prefWidth="250.0" text="start date:" />
                <TableColumn fx:id="tableColumnCreationDate" prefWidth="250.0" text="creation date:" />
                <TableColumn fx:id="tableColumnReserve" prefWidth="250.0" text="Rezerva" />


            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
        <HBox>
            <Button fx:id="buttonPrev" onAction="#handlePrevious" text="Prev"/>
            <Button fx:id="buttonNext" onAction="#handleNext" text="next"/>
        </HBox>
    </VBox>

</AnchorPane>
